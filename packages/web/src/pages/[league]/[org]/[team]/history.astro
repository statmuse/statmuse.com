---
import TeamFranchiseProfile from '@components/team-franchise-profile.astro'
import { Caching } from '@lib/caching'
import type { LeagueParam } from '@lib/params'
import type { NOT_FOUND_404 } from '@lib/response'

const team = Astro.params.team as string
const leagueParam = Astro.params.league as LeagueParam
const org = Astro.params.org as string
if (org !== "team" && org !== "club") return NOT_FOUND_404

Caching.swr(Astro)
---

<TeamFranchiseProfile league={leagueParam} {team} />
