---
/**
 *
 *  Legacy Route
 *
 *  GET /mlb/team/1948-st.-louis-cardinals-67/1948/splits
 *
 *  Redirect: GET /mlb/team/1948-st.-louis-cardinals-67/splits/1948
 *
 */

const league = Astro.params.league
const team = Astro.params.team
const year = Astro.params.year
const org = Astro.params.org
if (org !== "team" && org !== "club") return NOT_FOUND_404

return Astro.redirect(`/${league}/${org}/${team}/splits/${year}`)
---
