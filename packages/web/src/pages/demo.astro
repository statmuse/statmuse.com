---
import Header from '@components/header.astro'
import Layout from '@layouts/main.astro'
import Container from '@components/container.astro'

export const prerender = true

const meta = {
  title: 'Demo Video',
}
---

<Layout {meta}>
  <Header />
  <Container>
    <iframe
      class="aspect-video my-16"
      src="https://player.vimeo.com/video/220856612?color=0074d9&title=0&byline=0&portrait=0"
      width="100%"
      allowfullscreen
    >
    </iframe>
  </Container>
</Layout>
